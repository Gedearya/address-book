{
  "name": "gede-contacts-testing",
  "version": "1.0.0",
  "description": "Unit testing for Gede Contacts Application",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "assets/scripts/*.js",
      "!assets/scripts/main.js"
    ]
  }
}
